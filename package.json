{
  "name": "mechanical-pencil",
  "version": "0.1.0",
  "description": "A DIY static site generator based on Gobble",
  "homepage": "https://github.com/fskreuz/mechanical-pencil",
  "author": {
    "name": "Joseph Descalzota",
    "email": "fskreuz@gmail.com",
    "url": "http://scratchpaper.tk/"
  },
  "keywords": [
    "gobble",
    "static",
    "site",
    "generator"
  ],
  "engines": {
    "node": ">= 5.5.0",
    "npm": ">= 3.3.12"
  },
  "dependencies": {
    "express": "^4.13.3",
    "fs-plus": "^2.8.1",
    "gobble": "^0.10.2",
    "gobble-cli": "^0.6.0",
    "gray-matter": "^2.0.2",
    "marked": "^0.3.5",
    "moment": "^2.11.2",
    "handlebars": "^4.0.5",
    "handlebars-layouts": "^3.1.3"
  },
  "scripts": {
    "start": "node server.js",
    "build": "gobble build public -f",
    "clean": "rm -rfv .gobble .gobble-build public",
    "purge": "rm -rfv .gobble .gobble-build public node_modules",
    "devel": "gobble"
  },
  "main": "server.js",
  "siteData": {
    "site": {
      "name": "Hello World!",
      "owner": "John Doe",
      "email": "text@example.com",
      "links": [{
        "name": "Home",
        "url": "/"
      }, {
        "name": "Twitter",
        "url": "https://twitter.com/"
      }, {
        "name": "Github",
        "url": "https://github.com/"
      }]
    }
  }
}

